<?xml version='1.0' encoding='utf-8'?>
<widget id="vn.com.kerryexpress.simplenote" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Simple Note</name>
    <description>
        A little tool support create note
    </description>
    <author email="sa@kerryexpress.com.vn" href="https://kerryexpress.com.vn">
        Tmone NVT
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
